/* 必须要设置的全局属性 */
html #layui_layer_skinjwb_askstylecss{display:none; position: absolute; width:1989px;}

/* 控制图文部分的居中 */
/*body .layer-ext-jwb_ask.layui-layer-dialog{text-align: center;}*/

/* 标题部分 */
body .layer-ext-jwb_ask .layui-layer-title{height: 51px; line-height: 51px; background:#fff; font-size: 16px; color:#000; text-align: left; border-bottom: 1px #cfcfcf solid; padding-left:20px;}

/* 内容部分--图标样式设置 */
body .layer-ext-jwb_ask .layui-layer-content{padding: 0;}
