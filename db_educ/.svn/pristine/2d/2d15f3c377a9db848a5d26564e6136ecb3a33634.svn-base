/* 按钮样式 */
body .layer-ext-jwb_ask .layui-layer-btn{text-align: center; padding:10px 0 35px 0;}
body .layer-ext-jwb_ask .layui-layer-btn a{width: 98px; height: 25px; line-height: 25px; border-radius: 0px; font-size: 12px; color: #fff; display: inline-block; border: none; padding: 0; margin: 0 9px;}
body .layer-ext-jwb_ask .layui-layer-btn .layui-layer-btn0{background: #27b2fb;}
body .layer-ext-jwb_ask .layui-layer-btn .layui-layer-btn1{background: #ff4a3f;}

/* 关闭按钮样式 */
body .layer-ext-jwb_ask .layui-layer-setwin{right:20px;top:18px;}
body .layer-ext-jwb_ask .layui-layer-setwin .layui-layer-close{background: url(shut_icon.png) no-repeat center center !important;}

/* 底部遮罩层样式 */
.layui-layer-shade{background-color: #192942 !important;opacity:0.6 !important; filter:alpha(opacity=60) !important;}

/* 自定义样式 */
/*询问框*/
body .layer-ext-jwb_ask .ask_infor{font-size: 14px; color: #192942; text-align: center;padding: 65px 0 0 0;}
