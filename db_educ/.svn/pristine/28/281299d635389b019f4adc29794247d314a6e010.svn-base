<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.db.service.AnswerCardService">
	<resultMap type="com.db.entity.AnswerCard" id="AnswerCardMap">
		<result column="ac_id" property="acId" jdbcType="INTEGER" />
		<result column="paper_id" property="paperId" jdbcType="INTEGER"/>
		<result column="student_id" property="studentId" jdbcType="INTEGER" />
		<result column="teacher_id" property="teacherId" jdbcType="INTEGER" />
		<result column="card_image" property="cardImage" jdbcType="VARCHAR" />
		<result column="score" property="score" jdbcType="INTEGER" />
		<result column="change_time" property="changeTime" />
		<result column="create_time" property="createTime" />
	</resultMap>
	<insert id="saveAnswerCard" parameterType="com.db.entity.AnswerCard">
	   insert into td_answer_card(paper_id,student_id,card_image,create_time
	      <if test="teacherId != null and teacherId != ''">
	      	<![CDATA[,teacher_id]]>
	      </if>
	      <if test="score != null and score != ''">
	      	<![CDATA[,score]]>
	      </if>
	       <if test="changeTime != null and changeTime != ''">
	      	<![CDATA[,change_time]]>
	      </if>
	   ) 
	         values(#{paperId},#{studentId},#{cardImage},#{createTime}
	          <if test="teacherId != null and teacherId != ''">
	      	<![CDATA[,#{teacherId}]]>
	      </if>
	      <if test="score != null and score != ''">
	      	<![CDATA[,#{score}]]>
	      </if>
	       <if test="changeTime != null and changeTime != ''">
	      	<![CDATA[,#{changeTime}]]>
	      </if>
	         ) 
	</insert>
</mapper>